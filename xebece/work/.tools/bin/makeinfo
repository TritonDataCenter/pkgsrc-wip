echo "<.> for arg in "$@"; do case "$arg" in --version) exit 1 ;; esac; done; /usr/pkgsrc/mk/gnu-config/missing makeinfo "$@"" >> $wrapperlog
for arg in "$@"; do case "$arg" in --version) exit 1 ;; esac; done; /usr/pkgsrc/mk/gnu-config/missing makeinfo "$@"
