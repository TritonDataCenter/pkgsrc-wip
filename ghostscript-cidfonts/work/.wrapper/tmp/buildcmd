case $arg in
"")
	;;
-[DILR]*|-Wl,-R*|-Wl,-*,/*)
	case $cmd in
	*" "$arg|*" "$arg" "*)
		;;
	*)
		shquote "$arg"; arg="$shquoted"
		cmd="$cmd $arg"
		;;
	esac
	;;
-Wl,-Bdynamic|-Wl,-Bstatic)
	shquote "$arg"; arg="$shquoted"
	cmd="$cmd $arg"
	libs="$libs $arg"
	;;
-l*)
	case $libs in
	*" "$arg)
		;;
	*)
		shquote "$arg"; arg="$shquoted"
		libs="$libs $arg"
		;;
	esac
	;;
*)
	shquote "$arg"; arg="$shquoted"
	cmd="$cmd $arg"
	;;
esac
